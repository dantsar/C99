Grammar

    0 $accept: statement $end

    1 statement: expr ';'
    2          | statement expr ';'

    3 expr: binop

    4 binop: binop ',' value
    5      | binop '=' value
    6      | binop '+' value
    7      | value

    8 value: IDENT
    9      | NUMBER
   10      | CHARLIT


